{
  "name": "header-react",
  "description": "",
  "version": "1.0.0",
  "oc": {
    "files": {
      "data": "server.js",
      "template": {
        "src": "app.js",
        "type": "oc-template-react"
      }
    },
    "parameters": {
      "name": {
        "default": "World",
        "description": "Your name",
        "example": "Jane Doe",
        "mandatory": false,
        "type": "string"
      },
      "bgcolor": {
        "default": "grey",
        "description": "background color",
        "example": "red",
        "mandatory": false,
        "type": "string"
      }
    }
  },
  "devDependencies": {
    "oc-template-react-compiler": "^5.0.2"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
}
